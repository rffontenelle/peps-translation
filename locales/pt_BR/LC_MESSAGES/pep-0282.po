#: ../pep-0282.txt:434
msgid ""
"Text logging level for the message (\"DEBUG\", \"INFO\", \"WARN\", "
"\"ERROR\", \"CRITICAL\")"
msgstr ""

#: ../pep-0282.txt:437
#, python-format
msgid "``%(pathname)s``"
msgstr ""

#: ../pep-0282.txt:437
msgid ""
"Full pathname of the source file where the logging call was issued (if "
"available)"
msgstr ""

#: ../pep-0282.txt:440
#, python-format
msgid "``%(filename)s``"
msgstr ""

#: ../pep-0282.txt:440
msgid "Filename portion of pathname"
msgstr ""

#: ../pep-0282.txt:442
#, python-format
msgid "``%(module)s``"
msgstr ""

#: ../pep-0282.txt:442
msgid "Module from which logging call was made"
msgstr ""

#: ../pep-0282.txt:444
#, python-format
msgid "``%(lineno)d``"
msgstr ""

#: ../pep-0282.txt:444
msgid "Source line number where the logging call was issued (if available)"
msgstr ""

#: ../pep-0282.txt:447
#, python-format
msgid "``%(created)f``"
msgstr ""

#: ../pep-0282.txt:447
msgid "Time when the LogRecord was created (``time.time()`` return value)"
msgstr ""

#: ../pep-0282.txt:450
#, python-format
msgid "``%(asctime)s``"
msgstr ""

#: ../pep-0282.txt:450
msgid "Textual time when the LogRecord was created"
